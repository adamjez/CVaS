@model CVaS.Web.Models.WebServiceStatusViewModel

@if (!Model.IsLocalMode && !Model.IsAnyAlgServerOnline)
{
<div class="message">
    <div id="admin-message" class="alert alert-danger alert-dismissible" role="alert">
        <button type="button" class="close" data-dismiss="alert">&times;</button>
        <div>
            Oh snap! <br />
            Currently there isn't connected any algortihm server. <br /> 
            For more information look at <a asp-controller="Admin" asp-action="Index" class="alert-link" asp-fragment="status">Admin Section</a>.
        </div>
    </div>
</div>
}
